	"version": "0.0.15",
	"scripts": {
		"vscode:prepublish": "npm run compile && npm run compile-web",
		"vsix": "vsce package --allow-star-activation --yarn",
		"compile": "npx webpack-cli --config extension.webpack.config --mode production",
		"compile-web": "npx webpack-cli --config extension-browser.webpack.config --mode none",
		"watch": "npx webpack-cli --config extension.webpack.config --mode none --watch",
		"watch-web": "npx webpack-cli --config extension-browser.webpack.config --mode none --watch --info-verbosity verbose"
	},
	"extensionDependencies": [
		"CloudStudio.common"
	],
