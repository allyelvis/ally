		"ui",
		"workspace"
	],
	"api": "none",
	"activationEvents": [
		"onStartupFinished",
		"onAuthenticationRequest:github",
		"onAuthenticationRequest:github-enterprise"
	],
	"capabilities": {
		"virtualWorkspaces": true,
		"untrustedWorkspaces": {
			"supported": true
		}
	},
